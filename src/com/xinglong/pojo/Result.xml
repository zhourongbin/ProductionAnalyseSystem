<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="com.xinglong.pojo">
	    <insert id="addResult" parameterType="Result" >
	        insert into result_  ( name ) values (#{name})    
	    </insert>
	    
	    <delete id="deleteResult" parameterType="Result" >
	        delete from result_ where id= #{id}   
	    </delete>
	    
	    <select id="getResult" parameterType="_int" resultType="Result">
	        select * from   result_   where id= #{id}    
	    </select>

	    <update id="updateResult" parameterType="Result" >
	        update result_  set name=#{name} where id=#{id}    
	    </update>
	    <select id="lisResult" resultType="Result">
	        select * from   result_      
	    </select>	    
	</mapper>

